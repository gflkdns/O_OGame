<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.miqt.o_ogame.activity.WiFIConnActivity">


    <EditText
        android:id="@+id/et_ip1"
        android:layout_width="69dp"
        android:layout_height="46dp"
        android:layout_alignBaseline="@+id/et_ip2"
        android:layout_alignBottom="@+id/et_ip2"
        android:layout_toStartOf="@+id/et_ip2"
        android:ems="10"
        android:inputType="number"
        android:maxLength="3"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/et_ip2"
        android:layout_width="68dp"
        android:layout_height="46dp"
        android:layout_alignBaseline="@+id/et_ip3"
        android:layout_alignBottom="@+id/et_ip3"
        android:layout_toStartOf="@+id/et_ip3"
        android:ems="10"
        android:inputType="number"
        android:maxLength="3"
        tools:layout_editor_absoluteX="18dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/et_ip3"
        android:layout_width="70dp"
        android:layout_height="46dp"
        android:layout_alignBaseline="@+id/et_ip4"
        android:layout_alignBottom="@+id/et_ip4"
        android:layout_toStartOf="@+id/et_ip4"
        android:ems="10"
        android:inputType="number"
        android:maxLength="3"
        tools:layout_editor_absoluteX="156dp"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/et_ip4"
        android:layout_width="64dp"
        android:layout_height="46dp"
        android:layout_alignBottom="@+id/bt_conn_tcp"
        android:layout_toStartOf="@+id/bt_conn_tcp"
        android:ems="10"
        android:inputType="number"
        android:maxLength="3"
        tools:layout_editor_absoluteX="229dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/bt_conn_tcp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:text="play"
        tools:layout_editor_absoluteX="297dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/tv_localhost"
        android:layout_width="311dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="8dp"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="283dp" />

    <ListView
        android:id="@+id/lv_dev"
        android:layout_width="541dp"
        android:layout_height="200dp"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.349"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.413"
        tools:layout_editor_absoluteX="-156dp" />
</RelativeLayout>
